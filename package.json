{
  "name": "gotsport-tracker-app",
  "version": "1.2.0",
  "main": "expo-router/entry",
  "overrides": {
    "diff": "^5.2.0",
    "node-forge": "^1.3.2"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "build": "npm run test && eas build",
    "build:apk": "npm run test && eas build --platform android --profile preview",
    "build:ios": "npm run test && eas build --platform ios --profile preview",
    "build:ios:device": "npm run test && eas build --platform ios --profile preview-device",
    "build:ios:prod": "npm run test && eas build --platform ios --profile production",
    "postinstall": "node scripts/apply-expo-gradle-fix.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.0",
    "expo-asset": "^12.0.12",
    "expo-build-properties": "~0.13.0",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "node-html-parser": "^6.1.13",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "eas-cli": "^16.32.0",
    "expo": "~52.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "jest-junit": "^16.0.0",
    "react-test-renderer": "18.3.1",
    "patch-package": "^8.0.1",
    "typescript": "~5.3.0"
  },
  "private": true
}
