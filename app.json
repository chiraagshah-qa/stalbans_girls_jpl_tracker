{
  "expo": {
    "owner": "chiraagshah",
    "name": "St Albans City Academy - GotSport Tracker",
    "slug": "gotsport-tracker-app",
    "version": "1.3.0",
    "scheme": "gotsport",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/st-albans.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/st-albans.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "stalbans.gotsport.tracker",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/st-albans.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "stalbans.gotsport.tracker",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "targetSdkVersion": 35,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "extra": {
      "eventId": "46915",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dc1cca18-c647-4ecd-a628-aca630f0488d"
      }
    }
  }
}
