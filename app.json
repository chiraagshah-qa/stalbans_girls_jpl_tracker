{
  "expo": {
    "owner": "chiraagshah",
    "name": "St Albans City Academy - GotSport Tracker",
    "slug": "gotsport-tracker-app",
    "version": "1.0.0",
    "scheme": "gotsport",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/st-albans.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/st-albans.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gotsport.tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/st-albans.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.gotsport.tracker"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dc1cca18-c647-4ecd-a628-aca630f0488d"
      }
    }
  }
}
